    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug 'web_api'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=web_api",
                    "--package=web_api"
                ],
                "filter": {
                    "name": "web_api",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {"SHARED_VOLUME_NAME": "/web_api",
                    "VROOT_IMAGE": "bws-mgr/vroot:latest",
                    "COMMON_NETWORK": "bws-mgr-net"}
        },
        {
            "command": "npm run dev",
            "name": "Run npm dev",
            "cwd": "${workspaceFolder}/web_api/public",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "test 'db_controller'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=db_controller"
                ],
                "filter": {
                    "name": "db_controller",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
    ]
}